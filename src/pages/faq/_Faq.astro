---
import Paragraph from "../../components/typography/Paragraph.astro";

export type Props = {
  question: string;
  answer: string;
};

const { question, answer } = Astro.props;
---

<div class="space-y-4">
  <Paragraph class="font-semibold">{question}</Paragraph>
  <Paragraph>{answer}</Paragraph>
</div>
